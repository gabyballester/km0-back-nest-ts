  "scripts": {
    "start:dev": "node scripts/timed-run.js \"dotenv -e .env.development -- cross-env NODE_ENV=development npx nest start --watch --builder swc --preserveWatchOutput\" \"üîÑ Inicio completo con watch (Development)\"",
    "start:prod": "node dist/main",
    "build": "npx nest build --builder swc",
    "build:prod": "npx nest build --builder swc",
    "test:quick:ultra": "node scripts/timed-run.js \"cross-env NODE_ENV=test jest --maxWorkers=8 --bail --passWithNoTests --silent --no-coverage\" \"‚ö° Tests ultra r√°pidos (Test)\"",
    "test:full:ultra": "node scripts/timed-run.js \"cross-env NODE_ENV=test JEST_COVERAGE=true jest --coverage --maxWorkers=6 --bail --passWithNoTests --silent\" \"üìä Tests completos ultra r√°pidos con coverage (Test)\"",
    "test:watch": "cross-env NODE_ENV=test jest --watch --maxWorkers=2 --silent",
    "test:debug": "node scripts/timed-run.js \"cross-env NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand\" \"üêõ Tests con debug mode\"",
    "test:e2e:full": "node scripts/timed-run.js \"cross-env NODE_ENV=test jest --config ./test/jest-e2e.json --maxWorkers=1 --bail --silent\" \"üìä E2E tests completos (Test)\"",
    "type-check": "tsc --noEmit",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix --cache --max-warnings=0",
    "lint:check": "eslint \"{src,apps,libs,test}/**/*.ts\" --cache --max-warnings=0",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" --cache",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\" --cache",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "db:dev": "dotenv -e .env.development -- prisma db push --accept-data-loss",
    "db:test": "dotenv -e .env.test -- prisma db push --accept-data-loss",
    "db:prod": "node scripts/drizzle-production-deploy.js",
    "db:studio:dev": "dotenv -e .env.development -- prisma studio",
    "db:studio:test": "dotenv -e .env.test -- prisma studio",
    "db:studio:prod": "dotenv -e .env -- prisma studio",
    "db:check:ssl": "node scripts/check-ssl-config.js",
    "check:dependencies": "node scripts/check-dependencies.js",
    "fix:dependencies": "node scripts/check-dependencies.js fix",
    "check:ports": "node scripts/check-port-config.js"
  },
