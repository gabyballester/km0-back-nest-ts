{
  "name": "km0-market",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build --builder swc",
    "build:prod": "nest build --builder swc --webpack",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" --cache",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\" --cache",
    "start": "nest start",
    "start:dev": "nest start --watch --builder swc --preserveWatchOutput",
    "start:debug": "nest start --debug --watch --builder swc",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix --cache --max-warnings=0",
    "test": "jest --maxWorkers=2 --bail --passWithNoTests --silent",
    "test:fast": "jest --maxWorkers=4 --bail --passWithNoTests --silent --no-coverage",
    "test:watch": "jest --watch --maxWorkers=2 --silent",
    "test:cov": "jest --coverage --maxWorkers=2 --bail --passWithNoTests --silent",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --maxWorkers=1 --bail --silent",
    "test:e2e:fast": "jest --config ./test/jest-e2e.json --maxWorkers=1 --bail --silent --testTimeout=3000",
    "env:setup": "cp env.mirror .env",
    "env:example": "cp env.example .env",
    "check:process-env": "node scripts/check-process-env.js",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.ts": [
      "npm run format",
      "npm run lint",
      "npm run test:fast",
      "npm run check:process-env"
    ],
    "*.{json,md,yml,yaml}": [
      "npm run format"
    ]
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/throttler": "^6.4.0",
    "helmet": "^8.1.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "zod": "^4.0.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.5",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.3.96",
    "@swc/jest": "^0.2.29",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "globals": "^16.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "typescript-eslint": "^8.20.0"
  }
}
