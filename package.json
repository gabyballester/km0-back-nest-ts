{
  "name": "km0-market",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "start:dev": "dotenv -e .env.development -- cross-env NODE_ENV=development npx nest start --watch --preserveWatchOutput",
    "start:prod": "node -r ./tsconfig-paths-bootstrap.js dist/main",
    "build": "tsc -p tsconfig.build.json && node scripts/clean-test-files.js",
    "build:swc": "nest build --builder swc && node scripts/clean-test-files.js",
    "test:quick:ultra": "cross-env NODE_ENV=test jest --maxWorkers=8 --bail --passWithNoTests --silent --no-coverage",
    "test:full:ultra": "cross-env NODE_ENV=test JEST_COVERAGE=true jest --coverage --maxWorkers=6 --bail --passWithNoTests --silent",
    "test:watch": "cross-env NODE_ENV=test jest --watch --maxWorkers=2 --silent",
    "test:debug": "cross-env NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e:full": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json --maxWorkers=1 --bail --silent",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix --cache --max-warnings=0",
    "lint:check": "eslint \"{src,apps,libs,test}/**/*.ts\" --cache --max-warnings=0",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" --cache",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\" --cache",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "db:dev": "dotenv -e .env.development -- prisma db push --accept-data-loss",
    "db:dev:safe": "dotenv -e .env.development -- node scripts/drizzle-development-deploy.js",
    "db:dev:reset": "dotenv -e .env.development -- node scripts/drizzle-development-reset.js",
    "db:test": "dotenv -e .env.test -- prisma db push --accept-data-loss",
    "db:prod": "node scripts/drizzle-production-deploy.js",
    "db:studio:dev": "dotenv -e .env.development -- prisma studio",
    "db:studio:test": "dotenv -e .env.test -- prisma studio",
    "db:studio:prod": "dotenv -e .env -- prisma studio",
    "db:check:ssl": "node scripts/check-ssl-config.js",
    "migration:status": "node scripts/migration-manager.js status",
    "migration:generate": "node scripts/migration-manager.js generate",
    "migration:apply": "node scripts/migration-manager.js apply",
    "migration:reset": "node scripts/migration-manager.js reset",
    "migration:validate": "node scripts/migration-manager.js validate",
    "migration:workflow": "node scripts/migration-workflow.js",
    "migration:full": "node scripts/migration-manager.js full",
    "db:safety:check": "node scripts/database-safety-guard.js check",
    "db:safety:execute": "node scripts/database-safety-guard.js execute",
    "db:safety:backup": "node scripts/database-safety-guard.js backup",
    "db:safety:status": "node scripts/database-safety-guard.js status",
    "check:dependencies": "node scripts/check-dependencies.js",
    "fix:dependencies": "node scripts/check-dependencies.js fix",
    "check:ports": "node scripts/check-port-config.js",
    "update:imports": "node scripts/update-imports.js",
    "check:dist": "node scripts/check-dist-folders.js",
    "clean:unnecessary-dist": "node scripts/clean-unnecessary-dist.js",
    "test:compile:swc": "npx swc src -d dist-try/swc",
    "test:compile:tsc": "npx tsc -p tsconfig.json --outDir dist-try/tsc",
    "clean:test": "rm -rf dist-try"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/throttler": "^6.4.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@prisma/client": "^6.12.0",
    "@swc/core": "^1.3.96",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.3",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.3",
    "helmet": "^8.1.0",
    "postgres": "^3.4.7",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "tsconfig-paths": "^4.2.0",
    "zod": "^4.0.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.5",
    "@swc/cli": "^0.6.0",
    "@swc/jest": "^0.2.29",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/pg": "^8.15.4",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^9.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "glob": "^11.0.3",
    "globals": "^16.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^16.1.2",
    "prettier": "^3.0.0",
    "prisma": "^6.12.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "typescript-eslint": "^8.20.0"
  }
}
